{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@islands/*": ["src/islands/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowImportingTsExtensions": true
  }
}
